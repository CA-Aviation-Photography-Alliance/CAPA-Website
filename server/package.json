{
  "name": "capa-website-server",
  "version": "1.0.0",
  "description": "Express server for CAPA Website",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "setup-db": "node setup-mongodb.js",
    "config-local": "node configure-local-db.js",
    "test-api": "node test-simple-events.js",
    "test-auth": "node test-auth0-api.js",
    "test-creator-auth": "node test-creator-auth.js",
    "create-data": "node create-test-data.js",
    "db:start": "brew services start mongodb/brew/mongodb-community || sudo systemctl start mongod || net start MongoDB",
    "db:stop": "brew services stop mongodb/brew/mongodb-community || sudo systemctl stop mongod || net stop MongoDB",
    "db:status": "mongosh --eval \"db.runCommand({ ping: 1 })\" || echo \"MongoDB not running\"",
    "db:connect": "mongosh capa-events",
    "db:backup": "mongodump --db capa-events --out ./backup",
    "db:restore": "mongorestore --db capa-events ./backup/capa-events"
  },
  "dependencies": {
    "auth0": "^4.27.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.5.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.0",
    "node-fetch": "^3.3.2"
  },
  "overrides": {
    "lodash": "^4.17.21"
  },
  "keywords": [
    "express",
    "server",
    "api"
  ],
  "author": "",
  "license": "ISC"
}
